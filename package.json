{
  "name": "monts",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^28.1.3",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  }
}

{
  "name": "monts",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "prisma:save": "prisma migrate save --experimental",
    "prisma:up": "prisma migrate up --experimental",
    "prisma:generate": "prisma generate",
    "plop:generate": "yarn plop --plopfile .plop/plop.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@hookform/error-message": "^0.0.5",
    "@hookform/resolvers": "^1.3.2",
    "@prisma/client": "^2.14.0",
    "@rebass/forms": "^4.0.6",
    "@rebass/preset": "^4.0.5",
    "bcrypt": "^5.0.0",
    "date-fns": "^2.16.1",
    "lodash.merge": "^4.6.2",
    "next": "10.0.5",
    "next-iron-session": "^4.1.10",
    "next-pwa": "^5.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-hook-form": "^6.14.1",
    "react-toastify": "^6.2.0",
    "rebass": "^4.0.7",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2",
    "swr": "^0.4.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@prisma/cli": "^2.14.0",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rebass": "^4.0.7",
    "@types/rebass__forms": "^4.0.4",
    "@types/styled-components": "^5.1.7",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=4.3.7",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": ">=10.5.3",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "license": "ISC"
}
